{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:renderer dev:electron",
    "dev:renderer": "vite --config renderer/vite.config.js",
    "predev:electron": "pnpm run prepare:electron",
    "dev:electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 node ./scripts/run-electron.mjs ./electron/main.js",
    "prestart": "pnpm run prepare:electron",
    "start": "node ./scripts/run-electron.mjs ./electron/main.js",
    "prepare:electron": "node ./scripts/prepare-electron.mjs",
    "rebuild:sqlite": "node ./scripts/rebuild-sqlite.mjs",
    "fetch:electron": "node ./scripts/fetch-electron.mjs",
    "build:renderer": "vite build --config renderer/vite.config.js",
    "rebuild:native": "electron-rebuild -f -w better-sqlite3",
    "build:app": "pnpm run build:renderer && pnpm run rebuild:native && pnpm dlx electron-builder -c electron-builder.yml",
    "build:mac": "pnpm run build:renderer && pnpm dlx electron-builder -m -c electron-builder.yml",
    "build:win": "pnpm run build:renderer && pnpm run rebuild:native && pnpm dlx electron-builder -w -c electron-builder.yml",
    "lint": "eslint \"renderer/src/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.{js,mjs}\" \"electron/**/*.{js,mjs}\" --no-error-on-unmatched-pattern",
    "format": "prettier -w .",
    "postinstall": "node ./scripts/postinstall.mjs",
    "health": "node -e \"console.log({ node:process.version, pnpm:require('child_process').execSync('pnpm -v').toString().trim(), electron:(()=>{try{return require('electron/package.json').version}catch{ return 'not-installed'}})(), abi:process.versions.modules })\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "electron": "^30.5.1",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rebuild": "^0.1.2",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.5"
  },
  "type": "commonjs",
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.2.0",
    "dayjs": "^1.11.18",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.543.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.1",
    "zod": "^4.1.5"
  }
}