{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "prestart": "node ./scripts/fetch-electron.mjs",
    "predev": "node ./scripts/fetch-electron.mjs",
    "dev:css": "pnpm exec tailwindcss -i ./renderer/input.css -o ./renderer/tailwind.css -w",
    "dev:js": "node ./scripts/build-frontend.mjs --watch",
    "build:css": "pnpm exec tailwindcss -i ./renderer/input.css -o ./renderer/tailwind.css --minify",
    "build:js": "node ./scripts/build-frontend.mjs",
    "start": "./vendor/electron/Electron.app/Contents/MacOS/Electron .",
    "dev": "npm-run-all --parallel dev:css dev:js dev:app",
    "dev:app": "npm-run-all --parallel dev:main start",
    "dev:main": "nodemon --watch electron --ext js,json --exec \"./vendor/electron/Electron.app/Contents/MacOS/Electron .\"",
    "lint": "eslint . --ext .js,.mjs",
    "format": "prettier -w ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tailwindcss/cli": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "electron": "38.0.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rebuild": "^0.1.2",
    "tailwindcss": "^4.1.13"
  },
  "type": "commonjs",
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}