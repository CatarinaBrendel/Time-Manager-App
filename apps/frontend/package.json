{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "fetch:electron": "node ./scripts/fetch-electron.mjs",
    "prestart": "pnpm run fetch:electron",
    "predev": "pnpm run fetch:electron",
    "start": "electron apps/frontend/electron/main.js",
    "dev:main": "NODE_ENV=development nodemon --watch electron --ext js,json --exec \"node ./scripts/run-electron.mjs .\"",
    "dev:app": "npm-run-all --parallel dev:main start",
    "dev:js": "node ./scripts/build-frontend.mjs --watch",
    "build:js": "node ./scripts/build-frontend.mjs",
    "lint": "eslint \"renderer/src/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.mjs\" \"electron/**/*.mjs\" --no-error-on-unmatched-pattern",
    "format": "prettier -w .",
    "postinstall": "node ./scripts/postinstall.mjs",
    "rebuild:native": "node ./scripts/rebuild-native.mjs",
    "rebuild:sqlite": "npm_config_runtime=electron npm_config_target=38.0.0 npm_config_disturl=https://electronjs.org/headers npm_config_arch=arm64 pnpm rebuild better-sqlite3",
    "build:mac": "pnpm dlx electron-builder -m -c electron-builder.yml",
    "build:win": "pnpm dlx electron-builder -w -c electron-builder.yml",
    "build:app": "pnpm dlx electron-builder -c electron-builder.yml",
    "dev:renderer": "vite --config renderer/vite.config.js",
    "dev:electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron electron/main.js",
    "dev": "npm-run-all -p dev:renderer dev:electron",
    "build:renderer": "vite build --config renderer/vite.config.js",
    "build": "npm run build:renderer"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "electron": "38",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rebuild": "^0.1.2",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.5"
  },
  "type": "commonjs",
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
    "better-sqlite3": "^12.2.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.543.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.5"
  }
}